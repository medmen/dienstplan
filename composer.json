    "nyholm/psr7-server": "^1.1",
    "php-di/php-di": "^7.0",
    "monolog/monolog": "^3.7",
    "slim/php-view": "^3.4",
    "slim/flash": "^0.4.0",
    "odan/session": "^6.3",
